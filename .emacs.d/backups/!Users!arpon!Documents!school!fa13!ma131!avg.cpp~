#include <cstdlib>
#include <cstdio>
using namespace std;

FILE *in = fopen("raw-grades.txt", "r"),
    *out = fopen("avg-grades.txt", "w");

const int N = 19, M = 12, L = 50;

int total[M];
double grade[N][M];
char name[N][2][L];

int main()
{
    for (int i = 0; i < M; i++)
        fscanf(in, "%d", total + i);

    for (int i = 0; i < N; i++)
    {
        for (int j = 0; j < 2; j++)
            fscanf(in, "%s", name[i][j]);

        int drop = -1;
        for (int j = 0, g; j < M; j++)
        {
            fscanf(in, "%d", &g);
            grade[i][j] = (double) g / total[j];
            if (drop == -1 || grade[i][j] < grade[i][drop])
                drop = j;
        }

        double sum = 0.0;
        for (int j = 0; j < M; j++)
            if (j != drop)
                sum += grade[i][j];
        int avg = (int) (100.0 * sum / (M - 1) + 0.5);

        fprintf(out, "%s %s %d\n", name[i][0], name[i][1], avg);
    }

    return 0;
}
