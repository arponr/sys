\theoremstyle{definition}
\newtheorem{lemma}{Lemma}

\setlist{leftmargin=20pt, itemsep=2pt, topsep=2pt}
\setlist[enumerate, 1]{label=(\textup{\arabic*}), ref=\theequation.\arabic*}
\setlist[enumerate, 2]{label=\textit{\alph*}., ref=\theequation.\alph*}
\renewcommand{\labelitemi}{$\vcenter{\hbox{\footnotesize$\bullet$}}$}
\renewcommand{\labelitemii}{--}

\newcommand{\prob}[1]{\section*{Problem #1}}
\newcommand{\subprob}[1]{\subsection*{(#1)}}

\newcommand{\titleset}[3]{
  \title{Math 223a, Problem Set #1}
  \author{Arpon Raksit}
  \thanks{Due #2.
    \ifthenelse{\equal{#3}{}}{}{I discussed problems with #3.}
  }

  \maketitle
  \thispagestyle{fancy}
}
